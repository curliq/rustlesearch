{
  "name": "client-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "dayjs": "1.8.17",
    "lodash": "4.17.15",
    "ramda": "0.26.1",
    "superagent": "5.1.0",
    "tiny-date-picker": "3.2.8",
    "vue": "2.6.10",
    "vue-notification": "1.3.20",
    "vue-router": "3.1.3",
    "vue-select": "3.2.0",
    "vuex": "3.1.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@vue/cli-plugin-babel": "3.11.0",
    "@vue/cli-plugin-eslint": "3.11.0",
    "@vue/cli-service": "3.11.0",
    "@vue/eslint-config-airbnb": "4.0.1",
    "@vue/eslint-config-prettier": "5.0.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.6.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-vue": "6.0.0",
    "lint-staged": "9.4.2",
    "postcss-import": "12.0.1",
    "sass": "1.23.3",
    "sass-loader": "8.0.0",
    "tailwindcss": "1.1.3",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/airbnb",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not op_mini all",
    "not dead",
    "not ie > 0"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
