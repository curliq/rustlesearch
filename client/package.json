{
  "name": "client-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "dayjs": "1.8.25",
    "lodash": "4.17.15",
    "lodash-es": "4.17.15",
    "ramda": "0.27.0",
    "superagent": "5.2.2",
    "tiny-date-picker": "3.2.8",
    "vue": "2.6.11",
    "vue-notification": "1.3.20",
    "vue-router": "3.1.6",
    "vue-select": "3.10.1",
    "vue-text-highlight": "2.0.10",
    "vue-the-mask": "0.11.1",
    "vuex": "3.2.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "2.1.2",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-eslint": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/eslint-config-airbnb": "5.0.2",
    "@vue/eslint-config-prettier": "6.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-vue": "6.2.2",
    "lint-staged": "10.1.7",
    "postcss-import": "12.0.1",
    "sass": "1.26.5",
    "sass-loader": "8.0.2",
    "tailwindcss": "1.3.5",
    "vue-template-compiler": "2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/airbnb",
      "@vue/prettier"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "not op_mini all",
    "not dead",
    "not ie > 0"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
